@model IEnumerable<AuditTable>

<div class="container p-3">
    <h2 class="text-primary pt-3 pb-2 text-center">Patient Audit Table</h2>
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Patient Identifier</th>
                <th>Message GUID</th>
                <th>Message Sending System</th>
                <th>Received DateTime</th>
                <th>Message Text Link</th>
                <th>Is WebHook Send</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="5%">
                        @obj.ID
                    </td>
                    <td width="10%">
                        @obj.PatientIdentifier
                    </td>
                    <td width="20%">
                        @obj.MessageGUID
                    </td>
                    <td width="15%">
                        @obj.MessageSendingSystem
                    </td>
                    <td width="20%">
                        @obj.ReceivedDateTime
                    </td>
                    <td width="20%">
                        @obj.MessageTextLinktoUnstructuredStorage 
                    </td> 
                    <td width="10%">
                        @obj.IsWebHookSend
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>